|--------------------------------------------------\\n  */\\n  passedFromChild = (i, state) => {\\n    let arr = this.state.approved;\\n    arr.push(this.state.voucherDetails[i]);\\n\\n    this.setState({ rowSelected: state });\\n  };\\n\\n  /**\\n  |--------------------------------------------------\\n  | add a voucher\\n  |--------------------------------------------------\\n  */\\n  addVoucher = () => {\\n    this.setState({ addVoucher: false });\\n\\n    fetch(`${url}/api/vouchers`, {\\n      method: \\\"POST\\\",\\n      headers: {\\n        \\\"Content-Type\\\": \\\"application/json; charset=utf-8\\\",\\n        Accept: \\\"application/json\\\"\\n      },\\n      body: JSON.stringify({\\n        name: this.state.name,\\n        description: this.state.desc,\\n        amount: parseInt(this.state.amount),\\n        quantity: parseInt(this.state.quantity),\\n        expiry: this.state.expiry\\n      })\\n    })\\n      .then(res => res.json())\\n\\n      .then(data => {\\n        if (data.success === true) {\\n          alert(data.message);\\n          window.location.reload();\\n        }\\n      })\\n\\n      .catch(err =>\\n        console.log(\\\"adding a voucher function error on the voucher page\\\", err)\\n      );\\n  };\\n\\n  /**\\n  |--------------------------------------------------\\n  | get all voucher info and display it\\n  |--------------------------------------------------\\n  */\\n  getVouchers = () => {\\n    fetch(`${url}/api/vouchers`, {\\n      method: \\\"GET\\\",\\n      mode: \\\"cors\\\",\\n      headers: {\\n        \\\"Access-Control-Allow-Origin\\\": \\\"*\\\",\\n        \\\"Content-Type\\\": \\\"application/json; charset=utf-8\\\",\\n        Accept: \\\"application/json\\\"\\n      }\\n    })\\n      .then(res => res.json())\\n      .then(data => {\\n        if (data.success) {\\n          let vouchers = [];\\n          let voucherDetails = [];\\n\\n          data.vouchers.map(x => {\\n            let user = {\\n              name: 